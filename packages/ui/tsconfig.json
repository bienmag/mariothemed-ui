{
  "extends": "tsconfig/nextjs.json",
  "compilerOptions": {
    "incremental": false,
    "outDir": "dist",
    "rootDir": ".",
    "paths": {
      "@ui/*": ["./src/*"]
    },
    "target": "ES6",
    "jsx": "react-jsx",
    "jsxImportSource": "@emotion/react"
  },
  "include": ["."],
  "exclude": ["dist", "build", "node_modules"]
}
